<p id="notice"><%= notice %></p>

<h1>Killteams</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Requisition</th>
      <th>Assetcap</th>
      <th>Base</th>
      <th>Factionkeyword</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @killteams.each do |killteam| %>
      <tr>
        <td><%= killteam.name %></td>
        <td><%= killteam.description %></td>
        <td><%= killteam.requisition %></td>
        <td><%= killteam.assetcap %></td>
        <td><%= killteam.base %></td>
        <td><%= killteam.factionkeyword %></td>
        <td><%= link_to 'Show', killteam %></td>
        <td><%= link_to 'Edit', edit_killteam_path(killteam) %></td>
        <td><%= link_to 'Destroy', killteam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Killteam', new_killteam_path %>
